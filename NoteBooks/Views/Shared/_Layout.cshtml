<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title БЛОКНОТЫ</title>

    @Styles.Render("~/Content/Site.css")
    @Styles.Render("~/Content/font-awesome.css")
    @Styles.Render("~/Content/jquery.fancybox.css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>



<body class="body">

    <div class="navbar navbar-header navbar-fixed-top">
        <ul class="nav navbar-nav NavbarPadding">
            <li class="NavbarLi">@Html.ActionLink("О ПРОЕКТЕ", "About", "Home")</li>
            <li class="NavbarLi">@Html.ActionLink("ИЗДЕЛИЯ", "Product", "Home")</li>
            <li class="NavbarLi">@Html.ActionLink("ГАЛЕРЕЯ РАБОТ", "Gallery", "Home")</li>
            <li class="NavbarLi">@Html.ActionLink("КАК ЗАКАЗАТЬ", "Contact", "Home")</li>
        </ul>
    </div>


   


    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
